      <max-pool-size>5</max-pool-size> 
      <min-pool-size>1</min-pool-size> 
      <enable-freepool>false</enable-freepool> 
      <pool-waittimeout-millis>60000</pool-waittimeout-millis> 
      <low-activity-interval>60</low-activity-interval> 
      <is-secure>false</is-secure> 
      <is-clustered>true</is-clustered> 
      <server-session> 
	<max-messages>35</max-messages> 
	<min-pool-size>20</min-pool-size> 
	<max-pool-size>400</max-pool-size> 
      </server-session> 
      <thread-pool> 
	<min-pool-size>2</min-pool-size> 
	<max-pool-size>10</max-pool-size> 
