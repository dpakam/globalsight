<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.globalsight.cxe.entity.fileprofile.FileProfileImpl"
        table="FILE_PROFILE">
        <id name="id" column="ID" type="long">
            <generator class="native" />
        </id>
        <property name="codeSet" length="30" column="CODE_SET"
            type="string" not-null="false" unique="false" />
        <property name="description" length="4000" column="DESCRIPTION"
            type="string" not-null="false" unique="false" />
        <property name="knownFormatTypeId" column="KNOWN_FORMAT_TYPE_ID"
            type="long" not-null="true" unique="false" />
        <property name="l10nProfileId" column="L10N_PROFILE_ID"
            type="long" not-null="true" unique="false" />
        <property name="name" length="60" column="NAME" type="string"
            not-null="true" unique="false" />
        <property name="xmlRuleId" column="XML_RULE_ID" type="long"
            not-null="false" unique="false"/>
        <property name="companyId" column="COMPANYID" type="string"
            not-null="true" unique="false" />
        <property name="scriptOnImport" length="400"
            column="SCRIPT_ON_IMPORT" type="string" not-null="false"
            unique="false" />
        <property name="scriptOnExport" length="400"
            column="SCRIPT_ON_EXPORT" type="string" not-null="false"
            unique="false" />
        <property name="byDefaultExportStf" column="DEFAULT_EXPORT_STF"
            type="yes_no" not-null="true" unique="false" />
        <property name="isActive" column="IS_ACTIVE" type="yes_no"
            not-null="true" unique="false" />
        <property name="timestamp" column="TIMESTAMP" type="timestamp"
            not-null="true" />
        <property name="supportSid" column="IS_SID_SUPPORTED" type="yes_no"
            not-null="false" />
        <property name="unicodeEscape" column="IS_UNICODE_ESCAPE" type="yes_no"
            not-null="false" />
        <property name="headerTranslate" column="IS_HEADER_TRANSLATE" type="yes_no"
            not-null="false" />
        <property name="referenceFP" column="REFERENCE_FP" type="long"
            not-null="false" />
        <property name="newId" column="NEW_ID" type="long"
            not-null="false" />
            
        <property name="jsFilterRegex" length="255" column="JS_FILTER_REGEX" type="string"
            not-null="false" />
        <property name="filterId" column="FILTER_ID"
            type="long" not-null="false" unique="false" />
            
         <property name="filterTableName" length="45" column="FILTER_TABLE_NAME" type="string"
            not-null="false" />
        <set name="extensionIds" table="FILE_PROFILE_EXTENSION">
            <key column="FILE_PROFILE_ID" />
            <element column="EXTENSION_ID" type="long" />
        </set>
        
        <many-to-one name="xmlDtd" column="XML_DTD_ID"
            class="com.globalsight.cxe.entity.xmldtd.XmlDtdImpl"
            not-null="false" />
            
        <property name="terminologyApproval"  column="TERMINOLOGY_APPROVAL" 
          type="int" not-null="false" />
    </class>
</hibernate-mapping>