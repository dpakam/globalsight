<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.globalsight.everest.projecthandler.ProMTInfo"
		table="TM_PROFILE_PROMT_INFO">
		<id name="id" column="ID" type="long">
			<generator class="native" />
		</id>
		<property name="dirId" column="DIR_ID" type="long" not-null="false"
			unique="false" />
		<property name="dirName" column="DIR_NAME" length="40" type="string"
			not-null="false" unique="false" />
		<property name="topicTemplateId" column="TOPIC_TEMPLATE_ID"
			length="40" type="string" not-null="false" unique="false" />
		<many-to-one name="tmProfile" column="TM_PROFILE_ID"
			access="field"
			class="com.globalsight.everest.projecthandler.TranslationMemoryProfile"
			not-null="false" />

	</class>
</hibernate-mapping>