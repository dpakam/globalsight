<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.globalsight.everest.projecthandler.AsiaOnlineLP2DomainInfo"
		table="TM_PROFILE_AO_INFO">
		<id name="id" column="ID" type="long">
			<generator class="native" />
		</id>
		<property name="languagePairCode" column="LANGUAGE_PAIR_CODE"
			type="long" not-null="false" unique="false" />
		<property name="languagePairName" column="LANGUAGE_PAIR_NAME"
			length="50" type="string" not-null="false" unique="false" />
		<property name="domainCombinationCode" column="DOMAIN_COMBINATION_CODE"
			type="long" not-null="false" unique="false" />
		<many-to-one name="tmProfile" column="TM_PROFILE_ID"
			access="field"
			class="com.globalsight.everest.projecthandler.TranslationMemoryProfile"
			not-null="false" />

	</class>
</hibernate-mapping>