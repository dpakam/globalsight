<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.globalsight.everest.foundation.CountryCode"
        table="COUNTRY">
        <id name="ISO_COUNTRY_CODE" column="ISO_COUNTRY_CODE" type="string">
        </id>
    </class>
    <query name="queryCountry">
		FROM CountryCode country WHERE country.ISO_COUNTRY_CODE =:code
	</query>
</hibernate-mapping>
