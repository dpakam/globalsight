<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.globalsight.everest.projecthandler.TMProfileMTInfo"
        table="TM_PROFILE_MT_INFO">
        <id name="id" column="ID" type="long">
            <generator class="native" />
        </id>
        <property name="tmProfileID" 
            column="TM_PROFILE_ID" type="long" not-null="true"/>
            
        <many-to-one name="tmProfile" column="TM_PROFILE_ID" class="com.globalsight.everest.projecthandler.TranslationMemoryProfile" 
   			update="false" insert="false" not-found="ignore"/>
            
        <property name="mtEngine" length="50"
        	column="MT_ENGINE" type="string" not-null="false"/>
        	
        <property name="mtKey" length="50"
        	column="MT_KEY" type="string" not-null="false"/>
        	
        <property name="mtValue" length="200"
        	column="MT_VALUE" type="string" not-null="false"/>
        	
    </class>
</hibernate-mapping>
